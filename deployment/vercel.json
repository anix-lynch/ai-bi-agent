{
  "name": "ai-bi-agent",
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key",
    "HF_TOKEN": "@hf-token",
    "LANGSMITH_API_KEY": "@langsmith-api-key"
  }
}

